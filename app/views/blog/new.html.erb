<h1> Suggest New Blog </h2>
<%= form_with model: @new_blog do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>

    <br>
    <%= form.label :content %>
    <%= form.text_field :content %>

    <br>
    <%= form.submit 'Add Blog' %>
<% end %>
<%= link_to 'Back to List', blogs_path %>